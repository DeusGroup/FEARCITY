{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/culture",
      "destination": "/culture.html"
    },
    {
      "source": "/culture/",
      "destination": "/culture.html"
    }
  ]
}