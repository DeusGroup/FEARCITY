# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Backend Only - DO NOT expose these in frontend
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_keep_secret
SUPABASE_JWT_SECRET=your_jwt_secret_keep_secret

# Database Configuration - Backend Only
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres
PGUSER=postgres
PGPASSWORD=your_database_password
PGHOST=db.[YOUR-PROJECT-REF].supabase.co
PGPORT=5432
PGDATABASE=postgres

# Email Configuration (if using EmailJS)
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key

# Admin Configuration - Backend Only
ADMIN_PASSWORD=generate_secure_password_here