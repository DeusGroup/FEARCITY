{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/products/([^/]+)",
      "dest": "/api/products/[slug].js?slug=$1"
    },
    {
      "src": "/culture",
      "dest": "/culture.html"
    },
    {
      "src": "/culture/",
      "dest": "/culture.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}