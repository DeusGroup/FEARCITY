{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/products/([^/]+)",
      "destination": "/api/products/[slug].js?slug=$1"
    },
    {
      "source": "/culture",
      "destination": "/culture.html"
    }
  ]
}