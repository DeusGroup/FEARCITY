{
  "version": 2,
  "framework": null,
  "buildCommand": null,
  "outputDirectory": ".",
  "installCommand": null,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    {
      "source": "/culture",
      "destination": "/culture.html"
    },
    {
      "source": "/culture/",
      "destination": "/culture.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}