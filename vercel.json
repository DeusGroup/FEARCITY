{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/culture",
      "destination": "/culture.html"
    },
    {
      "source": "/culture/",
      "destination": "/culture.html"
    }
  ]
}