{
  "version": 2,
  "functions": {
    "api-health.js": {
      "runtime": "@vercel/node"
    },
    "api/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api-health.js"
    }
  ],
  "rewrites": [
    {
      "source": "/culture",
      "destination": "/culture.html"
    },
    {
      "source": "/culture/",
      "destination": "/culture.html"
    }
  ]
}