{
  "name": "fear-city-cycles-backend",
  "version": "0.1.4",
  "description": "Fear City Cycles E-commerce Backend with Square API Integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "prisma generate",
    "postinstall": "prisma generate",
